{
  "name": "@jimmychu0807/index-tpl-crud",
  "version": "0.0.2",
  "description": "Script to update index.html of Parity projects that publish their rustdocs in github.",
  "packageManager": "yarn@3.0.2",
  "main": "src/index.js",
  "homepage": "https://github.com/jimmychu0807/index-tpl-crud",
  "repository": {
    "url": "git+https://github.com/jimmychu0807/index-tpl-crud.git",
    "type": "git"
  },
  "bin": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "author": "Jimmy Chu <jimmychu0807@gmail.com>",
  "license": "WTFPL",
  "scripts": {
    "tsc:build": "tsc --build --clean && tsc --build",
    "copyfiles": "cp -r src/templates dist",
    "build": "yarn tsc:build; yarn copyfiles",
    "dev": "ts-node src/index.ts",
    "dev:debug": "yarn build; node --inspect-brk dist/index.js",
    "start": "dist/index.js",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src"
  },
  "dependencies": {
    "@types/node": "^16.10.5",
    "cash-dom": "^8.1.0",
    "commander": "^8.2.0",
    "ejs": "^3.1.6",
    "jsdom": "^18.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "ts-node": "^10.3.0",
    "typescript": "^4.4.4"
  },
  "bugs": {
    "url": "https://github.com/jimmychu0807/index-tpl-crud/issues"
  }
}
